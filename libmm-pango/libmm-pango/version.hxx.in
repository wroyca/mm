#pragma once

// The numeric version format is AAAAABBBBBCCCCCDDDE where:
//
// AAAAA - major version number
// BBBBB - minor version number
// CCCCC - bugfix version number
// DDD   - alpha / beta (DDD + 500) version number
// E     - final (0) / snapshot (1)
//
// When DDDE is not 0, 1 is subtracted from AAAAABBBBBCCCCC. For example:
//
// Version      AAAAABBBBBCCCCCDDDE
//
// 0.1.0        0000000001000000000
// 0.1.2        0000000001000020000
// 1.2.3        0000100002000030000
// 2.2.0-a.1    0000200001999990010
// 3.0.0-b.2    0000299999999995020
// 2.2.0-a.1.z  0000200001999990011
//
#define LIBMM_PANGO_VERSION       $libmm_pango.version.project_number$ULL
#define LIBMM_PANGO_VERSION_STR   "$libmm_pango.version.project$"
#define LIBMM_PANGO_VERSION_ID    "$libmm_pango.version.project_id$"
#define LIBMM_PANGO_VERSION_FULL  "$libmm_pango.version$"

#define LIBMM_PANGO_VERSION_MAJOR $libmm_pango.version.major$
#define LIBMM_PANGO_VERSION_MINOR $libmm_pango.version.minor$
#define LIBMM_PANGO_VERSION_PATCH $libmm_pango.version.patch$

#define LIBMM_PANGO_PRE_RELEASE   $libmm_pango.version.pre_release$

#define LIBMM_PANGO_SNAPSHOT_SN   $libmm_pango.version.snapshot_sn$ULL
#define LIBMM_PANGO_SNAPSHOT_ID   "$libmm_pango.version.snapshot_id$"
